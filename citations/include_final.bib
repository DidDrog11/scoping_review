	keywords = {Animals, *Rodentia, Virus Diseases/mi [Microbiology], Lassa virus/ip [Isolation \& Purification], *Disease Vectors, Sierra Leone, *RNA Viruses/ip [Isolation \& Purification], *Virus Diseases},
	pages = {263--265},
