	note = {WOS:000460803200091},
	keywords = {disease vectors, epidemiology, human diseases, strains, vectors},
	pages = {181},
	file = {Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\KJVCBCTY\\Diagne et al. - 2019 - Usutu Virus Isolated from Rodents in Senegal.pdf:application/pdf;Diagne et al. - 2019 - Supplementary:C\:\\Users\\ucbtds4\\Zotero\\storage\\DI95M94F\\Diagne et al. - 2019 - Supplementary.pdf:application/pdf}
