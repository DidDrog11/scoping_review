	pages = {609--613},
	file = {PubMed Central Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\33L2PWZ9\\Wulff et al. - 1975 - Recent isolations of Lassa virus from Nigerian rod.pdf:application/pdf;Wulff et al. - Recent isolations of Lassa virus from Nigerian rod.pdf:C\:\\Users\\ucbtds4\\Zotero\\storage\\9XTPBRHR\\Wulff et al. - Recent isolations of Lassa virus from Nigerian rod.pdf:application/pdf}
