	keywords = {animal ecology, attitudes, community action, community involvement, control methods, cost benefit analysis, costs, crop damage, habitats, knowledge level, pest control, population density, rodent control, rural areas, spatial distribution, sustainability, techniques, temporal variation, training, trapping},
