@article{dedet_ecology_nodate,
	title = {Ecology of a focus of cutaneous leishmaniasis in the {Thies} region ({Senegal}, {West} {Africa}). 4. - {Spontaneous} infestation and biology of {Phlebotomus} duboscqi {Neveu}-{Lemaire} 1906.},
