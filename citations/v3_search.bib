	note = {Place: ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES
Publisher: SPRINGER
Type: Article},
	keywords = {Phylogeny, Rodents, Trichurisspp., West Africa},
	pages = {1265--1271}
}

@article{adenyo_genome-wide_2017,
	title = {Genome-wide {DNA} markers to support genetic management for domestication and commercial production in a large rodent, the {Ghanaian} grasscutter ({Thryonomys} swinderianus)},
	volume = {48},
	issn = {0268-9146},
	doi = {10.1111/age.12478},
	abstract = {Domestication and commercial production of the grasscutter, Thryonomys swinderianus, a large rodent, represents an important opportunity to secure sustainable animal protein for local communities in West Africa. To support production, DNA markers are required for population diversity assessment, pedigree analysis and marker-assisted selection. This study reports the application of double-digest RAD sequencing to simultaneously discover and genotype SNP markers in 24 wild and recently domesticated grasscutters. An initial panel of 1209 SNP loci was characterised from a total of more than 21 000 candidate loci containing single SNPs. This genome-wide resource represents the first application of its type to commercial production of a large rodent for food and advances the use of agricultural genomics in Ghana.},
	language = {English},
	number = {1},
	journal = {ANIMAL GENETICS},
	author = {Adenyo, C. and Ogden, R. and Kayang, B. and Onuma, M. and Nakajima, N. and Inoue-Murayama, M.},
