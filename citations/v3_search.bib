	note = {Place: SACHSENPLATZ 4-6, PO BOX 89, A-1201 WIEN, AUSTRIA
Publisher: SPRINGER WIEN
Type: Article},
	keywords = {Arenavirid, Arenaviridae, Arenavirus, Bat virus, Bibdavirus, ICTV, Inclusion body disease, International Committee on Taxonomy of Viruses, Mammarenavirus, PASC, Reptarenavirus, Rodent virus, Snake virus, TaxoProp, Viral hemorrhagic fever, Virus classification, Virus nomenclature, Virus taxonomy},
	pages = {1851--1874}
