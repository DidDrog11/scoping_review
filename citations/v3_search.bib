	volume = {104},
	issn = {0027-8424},
	doi = {10.1073/pnas.0700531104},
	abstract = {Poxviruses (Poxviridae) are a family of double-stranded DNA viruses with no RNA stage. Members of the genus Orthopoxvirus (OPV) are highly invasive and virulent. It was recently shown that the taterapox virus (TATV) from a West African rodent is the sister of camelpox virus and therefore belongs to the clade closest to the variola virus (VARV), the etiological agent of smallpox. Although these OPVs are among the most dreaded pathogens on Earth, our current knowledge of their genomes, their origins, and their possible hosts is still very limited. Here, we report the horizontal transfer of a retroposon (known only from reptilian genomes) to the TATV genome. After isolating and analyzing different subfamilies of short interspersed elements (SINEs) from lizards and snakes, we identified a highly poisonous snake (Echis ocellatus) from West Africa as the closest species from which the SINE sequence discovered in the TATV genome (TATV-SINE) was transferred to the virus. We discovered direct repeats derived from the virus flanking the TATV-SINE, and the absence of any snake-derived DNA flanking the SINE. These data provide strong evidence that the TATV-SINE was actually transferred within the snake to the viral genome by retrotransposition and not by any horizontal transfer at the DNA level. We propose that the snake is another host for TATV, suggesting that VARV-related epidemiologically relevant viruses may have derived from our cold-blooded ancestors and that poxviruses are possible vectors for horizontal transfer of retroposons from reptiles to mammals.},
	language = {English},
	number = {29},
	journal = {Proceedings of the National Academy of Sciences of the United States of   America},
	author = {Piskurek, Oliver and Okada, Norihiro},
	month = jul,
	year = {2007},
	note = {Place: Okada, Norihiro; Tokyo Inst Technol, Fac Biosci and Biotechnol, 4259-B21   Nagatsuta-cho, Yokohama, Kanagawa 2268501, Japan, Japan.
ZOOREC:ZOOR14401000994},
	keywords = {Biochemistry, Diseases and   disorders, Echis ocellatus (Viperidae)., Echis ocellatus [Nucleic acids / Short interspersed elements in genome /   Horizontal transfer to taterapox virus genome, Genetics, implications for transfer   from reptiles to mammals] [Molecular genetics /  / ] [Viral diseases /   Tateropox virus / Horizontal transfer of host retroposons to pathogen   genome, implications for transfer to mammals]., Microbial diseases, Parasites diseases and disorders},
	pages = {12046--12051}
}

@article{fichet-calvet_fluctuation_2007-2,
	title = {Fluctuation of abundance and {Lassa} virus prevalence in {Mastomys} natalensis in {Guinea}, {West} {Africa}.},
	volume = {7},
	issn = {1530-3667},
	doi = {10.1089/vbz.2006.0520},
	abstract = {Based on empiric surveillance data, the incidence of human Lassa fever (LF) cases in Guinea and other West African countries has been reported to increase during the dry season compared to the rainy season. To investigate possible links with the ecology of the rodent reservoir of the virus, we conducted a 2-year longitudinal survey of Mastomys natalensis in a region of high human Lassa virus (LASV) seropositivity in Guinea. Standardized rodent trapping with similar trapping efforts between seasons was performed in three villages and 53.5\% (601/1123) of the animals were identified as M. natalensis using morphometric and molecular criteria. Mean trapping success (TS) of M. natalensis was always higher inside houses than in proximal cultivations. In the dry season, mean TS increased 2-fold inside houses and decreased up to 10-fold outside (p 0.0001), suggesting aggregation of rodents inside houses due to restricted food supply. 14.5\% (80/553) of M. natalensis were tested positive for Lassa virus by reverse transcription-polymerase chain reaction (RT-PCR; range, 5\%-30\%) and prevalence of the virus was two to three times higher in rodents captured in the rainy season than in the dry season (p 0.05). Inside houses, however, the LASV prevalence fluctuated nonsignificantly with season. These data suggest that in Guinea the risk of LASV transmission from rodents to humans is present both in the rainy and the dry season, reflected by the occurrence of LF cases throughout the year. In the dry season, however, the increased risk of humans encountering Mastomys and their excreta inside of houses may result in an increase of human Lassa fever cases.},
	language = {English},
	number = {2},
	journal = {Vector-Borne and Zoonotic Diseases},
	author = {Fichet-Calvet, Elisabeth and Lecompte, Emilie and Koivogui, Lamine and Soropogui, Barre and Dore, Amadou and Kourouma, Fode and Sylla, Oumar and Daffis, Stephane and Koulemou, Kekoura and Ter Meulen, Jan},
	year = {2007},
	note = {ZOOREC:ZOOR14311071472},
	keywords = {Ecology, Africa, Transmission, Diseases and disorders, Parasites diseases and disorders, Afrotropical region, Land zones, Population dynamics, Microbial   diseases, Habitat, Mammalia (Vertebrata).    Mastomys natalensis (Muridae)., Mammalia [Community structure /  / Small taxa] [Guinea / Faranah / ].    Mastomys natalensis [Viral diseases / Lassa virus / Seasonal prevalence, Man made   habitat, population size \& disease prevalence in human hosts relations]   [Transmission of viruses /  / Disease seasonal prevalence \& prevalence   in human hosts implications] [Population size /  / Viral disease   seasonal prevalence \& prevalence in human hosts relations] [Cultivated   land habitat /  / Viral disease seasonal prevalence] [Urban habitat /   Village houses / ] [Guinea / Faranah / ].},
	pages = {119--128}
}

@article{sbrana_comparative_2007-1,
	title = {Comparative pathology of {North} {American} and central {African} strains of monkeypox virus in a ground squirrel model of the disease.},
