	month = may,
	year = {2014},
	note = {Place: Dobigny, Gauthier; Ctr Biol Gest Populat UMR IRD INRA Cirad Montpell,   Inst Rech Dev, Campus Baillarguet CS30016, Montferrier Sur Lez, F-34988,   France, France.
ZOOREC:ZOOR15009046078},
	keywords = {Africa, Afrotropical   region, Habitat, Land zones, Man made habitat, Mastomys natalensis (Muridae)., Mastomys natalensis [Reproduction / Reproductive cycle / ] [Urban   habitat /  / ] [Niger / Niamey / ]., Reproduction},
	pages = {185--189}
}

