	note = {Place: THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND
Publisher: ELSEVIER SCI LTD
Type: Article},
	keywords = {Genetic diversity, Hammondia, Neospora caninum, Rodents, Toxoplasma gondii, West Africa},
	pages = {267--275}
}

