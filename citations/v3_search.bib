Publisher: WILEY-BLACKWELL
Type: Article},
	keywords = {Africa Occidental, bushmeat, carne silvestre, comercio, conservation planning, mamiferos, mammals, planificacion de la conservacion, trade, West Africa},
	pages = {1220--1228}
}

@article{singh_synthesis_2011,
	title = {Synthesis and in vitro and in vivo antimalarial activity of novel 4-anilinoquinoline {Mannich} base derivatives},
