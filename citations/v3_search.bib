	keywords = {*Chromosome Inversion, *Murinae/ge [Genetics], *Polymorphism, Genetic, Africa, Western, Animals, Animals, Wild/ge [Genetics], Cameroon, Chad, Chromosome Inversion/ge [Genetics], Chromosome Painting, Female, Gene Frequency, Geography, Male, Phylogeny},
