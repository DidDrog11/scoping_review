@article{goodman_natural_2016-2,
	series = {Erratum in: {Malar} {J}. 2016 {Nov} 17;15(1):560; {PMID}: 27855676 [https://www.ncbi.nlm.nih.gov/pubmed/27855676]},
	title = {Natural products from {Zanthoxylum} heitzii with potent activity against the malaria parasite.},
	volume = {15},
	issn = {1475-2875},
	doi = {10.1186/s12936-016-1533-x},
	abstract = {BACKGROUND: Zanthoxylum heitzii (Rutaceae) (olon) is used in traditional medicine in Central and West Africa to treat malaria. To identify novel compounds with anti-parasitic activity and validate medicinal usage, extracts and compounds isolated from this tree were tested against the erythrocytic stages of the human malaria parasite Plasmodium falciparum and for inhibition of transmission in rodent malaria parasite Plasmodium berghei., RESULTS: Hexane bark extract showed activity against P. falciparum (IC50 0.050 mug/ml), while leaf and seed extracts were inactive. Fractionation of the hexane bark extract led to the identification of three active constituents; dihydronitidine, pellitorine and heitziquinone. Dihydronitidine was the most active compound with an IC50 value of 0.0089 microg/ml (25 nM). This compound was slow acting, requiring 50 \% longer exposure time than standard anti-malarials to reach full efficacy. Heitziquinone and pellitorine were less potent, with IC50 values of 3.55 mug/ml and 1.96 microg/ml, but were fast-acting. Plasmodium berghei ookinete conversion was also inhibited by the hexane extract (IC50 1.75 microg/ml), dihydronitidine (0.59 microg/ml) and heitziquinone (6.2 microg/ml). Water extracts of Z. heitzii bark contain only low levels of dihydronitidine and show modest anti-parasitic activity., CONCLUSIONS: Three compounds with anti-parasitic activity were identified in Z. heitzii bark extract. The alkaloid dihydronitidine is the most effective of these, accounting for the bulk of activity in both erythrocytic and transmission-blocking assays. These compounds may present good leads for development of novel anti-malarials and add to the understanding of the chemical basis of the anti-parasitic activity in these classes of natural product.},
	number = {101139802},
	journal = {Malaria journal},
	author = {Goodman, Christopher Dean and Austarheim, Ingvild and Mollard, Vanessa and Mikolo, Bertin and Malterud, Karl Egil and McFadden, Geoffrey I and Wangensteen, Helle},
