	number = {Herbreteau, V.: Institut de Recherche pour le Developpement (IRD), 75231 Paris Cedex 05, France},
	journal = {Annals of the New York Academy of Sciences},
	author = {Herbreteau, V. and Gonzalez, J. P. and Hugot, J. P.},
	editor = {Blouin, E. F. and Maillard, J. C.},
	year = {2006},
	note = {Place: UK
Impact of Emerging Zoonotic Diseases and Animal Health: 8th Biennial Conference of the Society for Tropical Veterinary Medicine, Hanoi, Vietnam, 26 June-1 July 2005.},
	keywords = {biogeography, distribution, hosts, human diseases, infections, phylogenetics, reservoir hosts, serological surveys, viral diseases, wild animals, zoonoses},
	pages = {39--56}
}

@article{howie_gambian_2006,
