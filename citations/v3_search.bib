	author = {Sogoba, N. and Rosenke, K. and Adjemian, J. and Diawara, S. I. and Maiga, O. and Keita, M. and Konate, D. and Keita, A. S. and Sissoko, I. and Boisen, M. and Nelson, D. and Oottamasathien, D. and Millett, M. and Garry, R. F. and Branco, L. M. and Traore, S. F. and Doumbia, S. and Feldmann, H. and Safronetz, D.},
	year = {2016},
