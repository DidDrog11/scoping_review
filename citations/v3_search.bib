	journal = {The New England journal of medicine},
	author = {Reed, Kurt D and Melski, John W and Graham, Mary Beth and Regnery, Russell L and Sotir, Mark J and Wegner, Mark V and Kazmierczak, James J and Stratman, Erik J and Li, Yu and Fairley, Janet A and Swain, Geoffrey R and Olson, Victoria A and Sargent, Elizabeth K and Kehl, Sue C and Frace, Michael A and Kline, Richard and Foldy, Seth L and Davis, Jeffrey P and Damon, Inger K},
	year = {2004},
	note = {Place: United States
Reed, Kurt D. Department of Pathology, Marshfield Clinic, Marshfield, Wisc, USA. reed.kurt@mcrf.mfldclin.edu},
	keywords = {*Monkeypox virus/ip [Isolation \& Purification], *Monkeypox/vi [Virology], *Sciuridae/vi [Virology], Adolescent, Adult, Animals, Child, Child, Preschool, Disease Outbreaks, DNA, Viral/an [Analysis], Female, Humans, Male, Monkeypox virus/ge [Genetics], Monkeypox/ep [Epidemiology], Monkeypox/tm [Transmission], Monkeypox/ve [Veterinary], Muridae/vi [Virology], Polymerase Chain Reaction, Polymorphism, Restriction Fragment Length, Skin/vi [Virology], Wisconsin/ep [Epidemiology], Zoonoses/ep [Epidemiology], Zoonoses/tm [Transmission], Zoonoses/vi [Virology]},
