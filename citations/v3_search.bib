@article{sallam_revision_2020-1,
	title = {Revision of {Oligocene} '{Paraphiomys}' and an origin for crown {Thryonomyoidea} ({Rodentia}: {Hystricognathi}: {Phiomorpha}) near the {Oligocene}-{Miocene} boundary in {Africa}.},
	volume = {190},
	issn = {0024-4082},
	abstract = {'Paraphiomys' simonsi is a phiomorph rodent from the early Oligocene of Egypt ([approximately]29-30 Mya) that has historically been aligned with much younger ({\textless}[approximately]20 Mya) Miocene species of the genera Paraphiomys and Neosciuromys. Here, we use Bayesian tip-dating analysis of a 109-character morphological matrix containing 57 living and extinct ctenohystricans to test these proposed placements for Paraphiomys' simonsi. Our analyses provide support for the exclusion of 'Paraphiomys' simonsi from both Paraphiomys and Neosciuromys and justify the establishment of a new genus (Monamys gen. nov.) for this stem thryonomyoid. These analyses also indicate that the divergence of the extant dassie rat Petromus from the extant cane rat Thryonomys (i.e. origin of crown Thryonomyoidea) occurred [approximately]23.7 Mya, close to the Oligocene-Miocene boundary and in close agreement with recent molecular estimates for this split. Miocene Neosciuromys, Paraulacodus, Protohummus and the type species of Paraphiomys are identified as stem thryonomyids, whereas the Namibian species Apodecter strorneri, Tufamys woodi,Paraphiomys' australis and Paraphionzys' roessneri are identified for the first time as stem petromurids, raising the possibility of a long period of endemic petromurid evolution in south-west Africa. Comparison of molecular divergence estimates with our optimal tip-dated topology suggests that stem bathyergoids are most likely to have arisen from late Eocene and early Oligocene 'phiomyids'.},
	language = {English},
	number = {1},
	journal = {Zoological Journal of the Linnean Society},
	author = {Sallam, Hesham M. and Seiffert, Erik R.},
	month = sep,
	year = {2020},
	note = {Place: Seiffert, Erik R.; Univ Southern Calif, Dept Integrat Anat Sci, Los   Angeles, CA, 90007, USA, USA.
ZOOREC:ZOOR15703014374},
	keywords = {Caenozoic, Combination, Evolution, Fossil]., Fossil].    Monamys simonsi - (Wood 1968) (Rodentia): [Comb nov, Transferred from   Paraphiomys, p. 357, Fossil].    Thryonomyoidea (Rodentia).    Paraphiomys simonsi - Wood 1968 (Phiomyidae): [Designated type species   for new genus, Monamys, p. 357, General   morphology, Geological time periods, Monamys (Rodentia): [Gen nov, Of parvorder Phiomorpha, Type species   Paraphiomys simonsi Wood 1968, p. 357, Oligocene, Monamys [Oligocene /  / New genus].    Thryonomyoidea [General morphology / Morphological characters /   systematics] [Phylogeny /  / ]., New taxa, Nomenclature, Palaeogene, Systematics, Tertiary, Type species},
