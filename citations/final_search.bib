Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {animal diseases, disease prevalence, disease transmission, ecology, endemic hotspots, epidemiology, evolution, geographical distribution, host parasite relationships, host range, hosts, human diseases, Lassa fever, Lassa virus, outbreaks, phylogenetics, phylogeny, phylogeography, reservoir hosts, rodent abundance, rodent reservoirs, strains, taxonomy, viral diseases, virus evolution, wild animals, zoogeography, zoonoses},
	pages = {312},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\7NC7E9VR\\Olayemi and Fichet-Calvet - 2020 - Systematics, Ecology, and Host Switching Attribut.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\82NZEB8D\\312.html:text/html}
