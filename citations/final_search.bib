	keywords = {Africa, Afrotropical region, Dispersal, Evolution, Genetics, Land   zones, Means of dispersal, Rattus rattus (Muridae)., Zoogeography},
	pages = {1--11},
	file = {Etougbetche et al. - 2020 - Genetic diversity and origins of invasive black ra.pdf:C\:\\Users\\ucbtds4\\Zotero\\storage\\6YQB88EP\\Etougbetche et al. - 2020 - Genetic diversity and origins of invasive black ra.pdf:application/pdf}
