@article{udofia_response_2009,
	title = {Response surface analysis of toxic effect of {Azdirachta} indica on haematological indices and enzyme activity in female albino rats.},
	volume = {2},
	issn = {2006-3938},
	abstract = {Chemical control methods have been used for preservation of stored cereal and legume grains in Nigeria but this practice has caused some levels of toxicity among end consumers. To check the trend, Azdirachta indica seed powder has been used as substitute for synthetic insecticides. To test the safety of this plant product, 30 female albino rats of varying ages were fed ad libitum, with maize meal treated with 30 mg of A. indica seed powder per kg of rodent feed. The animals were sacrificed and their blood samples collected and analysed for packed cell volume (PVC), hemoglobin content (Hb), white blood cell (WBC), red blood cell (RBC), platelets, and liver function test of alkaline phosphatase and aspertate transaminase. We found that the age, dose, and period of exposure did not significantly affect standard levels of the studied parameters and maize meal treated with 30 mgKg-1 A. indica seed powder was safe. Thus, this plant product may be an economic and healthy alternative to synthetic insecticides for stored products.},
	language = {English},
	number = {3},
