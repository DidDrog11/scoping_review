@article{el-yuguda_peridomestic_2009,
	title = {Peridomestic rodents and {Lassa} fever virus infection of humans in urban and rural communities in {Borno} state, {Nigeria}.},
	volume = {30},
	issn = {0331-3026},
	url = {http://www.ajol.info/journal_index.php?jid=19&tran=0&ab=0},
	abstract = {The prevalence of peridomestic rodents and consumption of their meat as well as other human habits as possible risk factors for rodent to human transmission of Lassa fever virus (LFV) in rural and urban communities in Borno state, Nigeria was investigated. A structured questionnaire was administered to each household to determine the human-rodent contacts, while the prevalence of LFV antibody in rodents was evaluated by complement fixation (CF) test. The most frequent medical complaints among the respondents in the rural settlements were those of severe headache, backache and febrile illness with permanent or temporary hearing defects. The urban respondents reported more of sore throat, cough and epigastric pain. More rural respondents hunt for and eat rodent meat and leave their foodstuff open when compared to the same habits among urban respondents. Respondents from both communities reported higher rodent activities at night and during the dry season. A high prevalence of CF antibodies against LFV was detected among the rodent sera tested. Thirty six (55\%) out of the 65 positive sera had high titers ranging from 1:32 to 1:256. A relatively lower number (33\%) of sera from Rattus rattus had high CF antibody titers (=1:32) when compared to the Rattus norvegicus (57\%) and Cricetomys gambianus (79\%) species. The results of this study further emphasize the importance of rodents in the maintenance and spread of LFV infection in the study areas.},
	language = {English},
	number = {2},
	journal = {Nigerian Veterinary Journal},
	author = {El-Yuguda, A. D. and Baba, S. S. and Aliyu, A. M.},
	year = {2009},
	note = {Place: Nigeria},
	keywords = {disease prevalence, disease transmission, hosts, human diseases, Lassa fever, reservoir hosts},
	pages = {65--70}
}

@article{faleke_cutaneous_2008,
