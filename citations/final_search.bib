	pages = {21977},
	file = {Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\DX4YCGHU\\Fichet-Calvet et al. - 2016 - Spatial and temporal evolution of Lassa virus in t.pdf:application/pdf}
