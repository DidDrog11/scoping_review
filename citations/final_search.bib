	year = {2014},
	note = {Place: Netherlands},
	keywords = {children, cohort studies, detection, faecal examination, genome analysis, genomes, hosts, human diseases, identification, paediatrics, reservoir hosts},
	pages = {25--33}
}

@article{raufu_persistence_2014,
