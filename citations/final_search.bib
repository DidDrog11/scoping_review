	author = {Witkowski, Peter T. and Klempa, Boris and Ithete, Ndapewa L. and Auste, Brita and Mfune, John K. E. and Hoveka, Julia and Matthee, Sonja and Preiser, Wolfgang and Kruger, Detlev H.},
	month = jul,
