@article{killick-kendrick_parasitic_1984,
	title = {Parasitic protozoa of the blood of rodents. {VI}. {Two} new hemogregarines of the pygmy flying squirrel {Idiurus} macrotis ({Rodentia}: {Theridomyomorpha}: {Anomaluridae}) in {West} {Africa}.},
