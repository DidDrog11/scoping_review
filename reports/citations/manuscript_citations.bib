	author = {García-Peña, Gabriel E. and Rubio, André V. and Mendoza, Hugo and Fernández, Miguel and Milholland, Matthew T. and Aguirre, A. Alonso and Suzán, Gerardo and Zambrana-Torrelio, Carlos},
	month = nov,
	year = {2021},
	note = {Publisher: Royal Society},
	keywords = {Rodentia, disease risk, emerging zoonotic diseases, environmental change, land-use scenarios},
	pages = {20200362},
	file = {rstb.2020.0362.pdf:/Users/david/Zotero/storage/CCFWUXKI/rstb.2020.0362.pdf:application/pdf},
