	volume = {1},
	copyright = {© The Ecological Society of America},
	issn = {1540-9309},
	shorttitle = {Mice, rats, and people},
	url = {https://doi.org/10.1890/1540-9295(2003)001[0367:MRAPTB]2.0.CO;2},
	doi = {https://doi.org/10.1890/1540-9295(2003)001[0367:MRAPTB]2.0.CO;2},
	abstract = {Mice, rats, and other rodents threaten food production and act as reservoirs for disease throughout the world. In Asia alone, the rice loss every year caused by rodents could feed about 200 million people. Damage to crops in Africa and South America is equally dramatic. Rodent control often comes too late, is inefficient, or is considered too expensive. Using the multimammate mouse (Mastomys natalensis) in Tanzania and the house mouse (Mus domesticus) in southeastern Australia as primary case studies, we demonstrate how ecology and economics can be combined to identify management strategies to make rodent control work more efficiently than it does today. Three more rodent–pest systems – including two from Asia, the rice-field rat (Rattus argentiventer) and Brandt's vole (Microtus brandti), and one from South America, the leaf-eared mouse (Phyllotis darwini) – are presented within the same bio-economic perspective. For all these species, the ability to relate outbreaks to interannual climatic variability creates the potential to assess the economic benefits of forecasting rodent outbreaks.},
	language = {en},
	number = {7},
	urldate = {2021-02-01},
	journal = {Frontiers in Ecology and the Environment},
	author = {Stenseth, Nils Chr and Leirs, Herwig and Skonhoft, Anders and Davis, Stephen A. and Pech, Roger P. and Andreassen, Harry P. and Singleton, Grant R. and Lima, Mauricio and Machang'u, Robert S. and Makundi, Rhodes H. and Zhang, Zhibin and Brown, Peter R. and Shi, Dazhao and Wan, Xinrong},
	year = {2003},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/1540-9295\%282003\%29001\%5B0367\%3AMRAPTB\%5D2.0.CO\%3B2},
	pages = {367--375},
	file = {Full Text PDF:/Users/david/Zotero/storage/AMA3HL3T/Stenseth et al. - 2003 - Mice, rats, and people the bio-economics of agric.pdf:application/pdf;Full Text PDF:/Users/david/Zotero/storage/EDLRYCA9/Stenseth et al. - 2003 - Mice, rats, and people the bio-economics of agric.pdf:application/pdf;Snapshot:/Users/david/Zotero/storage/LI5LN4H4/1540-9295(2003)001[0367MRAPTB]2.0.html:text/html},
