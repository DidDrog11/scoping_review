	keywords = {Agriculture, communities, pests, Rodents, techniques, health, losses, public health, vertebrate pests, adverse effects, control methods, Metaanalysis, efficacy, crop losses, Crops, damage, data analysis, farming systems, feeding, feeding habits, guidelines, meta-analysis, methodology, Pest control, Population ecology, Predation, seedlings, Seedlings},
