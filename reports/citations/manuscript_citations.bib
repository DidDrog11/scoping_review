	keywords = {hantavirus, antibody prevalence, emerging and infectious disease, Montana, Peromyscus maniculatus, Sin Nombre virus},
	pages = {e01368},
	file = {Full Text PDF:/Users/david/Zotero/storage/A9IJJHIV/Douglass and Vadell - 2016 - How much effort is required to accurately describe.pdf:application/pdf;Full Text PDF:/Users/david/Zotero/storage/KWBX3VKT/Douglass and Vadell - 2016 - How much effort is required to accurately describe.pdf:application/pdf;Snapshot:/Users/david/Zotero/storage/2VALIWUK/ecs2.html:text/html;Snapshot:/Users/david/Zotero/storage/CMHPX4TU/ecs2.html:text/html},
