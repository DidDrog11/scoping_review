	author = {Galeh, Tahereh Mikaeili and Sarvi, Shahabeddin and Montazeri, Mahbobeh and Moosazadeh, Mahmood and Nakhaei, Maryam and Shariatzadeh, Seyyed Ali and Daryani, Ahmad},
	year = {2020},
	file = {Full Text PDF:/Users/david/Zotero/storage/FRUWZQGE/Galeh et al. - 2020 - Global Status of Toxoplasma gondii Seroprevalence .pdf:application/pdf},
}

@article{mcmahon_ecosystem_2018,
