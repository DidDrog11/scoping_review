	pmcid = {PMC4766397},
	pages = {21977},
	file = {Full Text:/Users/david/Zotero/storage/FQTDDEA2/Fichet-Calvet et al. - 2016 - Spatial and temporal evolution of Lassa virus in t.pdf:application/pdf},
}

@article{herrmann_accelerating_2020,
